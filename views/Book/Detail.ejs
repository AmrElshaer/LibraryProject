
<div>
    <label>Name</label>
    <input readonly name="Name" value="<%= book.Name %>" type="text">
</div>
<div>
    <label>Title</label>
    <input readonly name="Title" value="<%= book.Title %>" type="text">
</div>
<div>
    <label>ImageConver</label>
    <img height="150px" width="100" src="<%=book.ImagePath%>">
   
</div>
<div>
    <label>Author</label>
    <input readonly name="Author" value="<%-book.Author.Name%>"/>
       
    
   
</div>
<div>
    <label>PageCount</label>
    <input readonly name="PageCount" value="<%= book.PageCount %>" type="number">
</div>
<div>
    <label>PublshDate</label>
    <input readonly name="PublshDate" value="<%= book.PublshDate==null?'':book.PublshDate.toISOString().split('T')[0] %>" type="date">
</div>
<div>
    <label>Description</label>
    <textarea readonly name="Description" ><%= book.Description %></textarea>
</div>
